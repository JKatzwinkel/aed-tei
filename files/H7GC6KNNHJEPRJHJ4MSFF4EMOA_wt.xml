<ab>
<s corresp="src:tlaIBUBd6ibVokTcUPygErCj1kl7lY">
<w xml:lang="de" corresp="src:tlaIBUBd5S0ZWReAkpMtlq69Qg0M8U">Spruch</w>
<w xml:lang="de" corresp="src:tlaIBUBd47fFVVVGEhcpJwhciuCEG0">[Gen.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd8z1lArRIkGUuW616MDLg4I">gelangen nach, kommen zu</w>
<w xml:lang="de" corresp="src:tlaIBUBd5rAPqMaZEkmtZZbwxt1WdY">zu, bis, an, in [lokal]</w>
<w xml:lang="de" corresp="src:tlaIBUBd2lw7v4SEEagiWvYqTVJ7o0">ON/Rosetau</w>
</s>
<s corresp="src:tlaIBUBd5SebKM6Z00aqr7LRQQ8EnM">
<w xml:lang="de" corresp="src:tlaIBUBd6eEJZHLaUIxqv1tpyr1YXk">Worte sprechen</w>
<w xml:lang="de" corresp="src:tlaIBUBd8YXLVtQekkDgNM1XWUgNfI">durch, seitens jmds.</w>
</s>
<s corresp="src:tlaIBUBd13bK8vaq0TCgxyTP2WRhI4">
<w xml:lang="de" corresp="src:tlaIBUBd0bc5uZNyEaZiQdMs4Hy1Vg">ich, [pron. abs. 1. sg.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd15VxlNQiURoluwVCVaXhMY">gebären</w>
<w xml:lang="de" corresp="src:tlaIBUBdyQel7xRtEVUlMrPXfzCQWw">in, zu, an, aus [lokal]</w>
<w xml:lang="de" corresp="src:tlaIBUBd9ZzQNJ0B0Scufz289Jjybg">ON/Rosetau</w>
</s>
<s corresp="src:tlaIBUBdwabIGVGuEcatPhnL59NTOs">
<w xml:lang="de" corresp="src:tlaIBUBdwo97sdXr0AMktkmd01dXFc">geben</w>
<w xml:lang="de" corresp="src:tlaIBUBdzSw6slhWkA4tQ3k716XyV0">zu, für, an [Richtung]; [Dat.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd3yLkjMPmUKmh7g4pl9J6oo">ich [pron. suff. 1. sg.]</w>
<w xml:lang="de" corresp="src:tlaIBUBdxYFJf4MIEl2gNb9vO6PbMA">Herrlichkeit, Nützliches</w>
<w xml:lang="de" corresp="src:tlaIBUBdwiovxoHWEeCkZg9ZcDSn34">durch, seitens jmds.</w>
<w xml:lang="de" corresp="src:tlaIBUBd8nQUQQUaESapbYvU9mzqnc">Dazugehöriger, Bewohner</w>
<w xml:lang="de" corresp="src:tlaIBUBdwAPFJ5bVUkNkblH0QdhNGY">Würdenzeichen, Würde</w>
<w xml:lang="de" corresp="src:tlaIBUBd8Dz7HFagERhq7fImikW7C0">in (Freude), als (Gekochtes) [Zustand,Weise]</w>
<w xml:lang="de" corresp="src:tlaIBUBdzjzewlTekzgtLwqWw1VcQ0">Reinheit</w>
<w xml:lang="de" corresp="src:tlaIBUBdQBujqsPY0JUuUiyCQ7IWNk">GN/Osiris</w>
</s>
<s corresp="src:tlaIBUBdx0lv61cIUsHgYpeBTjJ1hk">
<w xml:lang="de" corresp="src:tlaIBUBdQbsaetyo0FQhuE86e2jn4U">empfangen, nehmen</w>
<w xml:lang="de" corresp="src:tlaIBUBd1mysEO3nU8ApaxI1YP4vsA">das Alter</w>
<w xml:lang="de" corresp="src:tlaIBUBdwivWJVN7EyojUoS1iKj8ZU">in, zu, an, aus [lokal]</w>
<w xml:lang="de" corresp="src:tlaIBUBd3Ab7OZGQkXgt6hbjnvwgAM">ON/Rosetau</w>
<w xml:lang="de" corresp="src:tlaIBUBd1to44sec0d1uUzvv43BgZo">als, wenn [Konjunktion]</w>
<w xml:lang="de" corresp="src:tlaIBUBd6WIx9dH8kUbgHPr478MA80">führen, leiten</w>
<w xml:lang="de" corresp="src:tlaIBUBd71mu9UuZUfWkWAMK96oiXA">GN/Osiris</w>
<w xml:lang="de" corresp="src:tlaIBUBd66d0wvW0Ew4tQPpH7u37ew">auf, über, vor, hinter [lok.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd1xe4xzJs0Nbmq9OP5xsG0M">Stätte; Hügel</w>
<w xml:lang="de" corresp="src:tlaIBUBdx4bkN8xckC2sTaUQEGE3Qc">GN/Osiris</w>
</s>
<s corresp="src:tlaIBUBd43M8LoyZk7ovwUjqcQQudk">
<w xml:lang="de" corresp="src:tlaIBUBdy8XcU0dl0QQl5sEno8tkZI">ich, [pron. abs. 1. sg.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd1mykWJEeEjBnqQqCPhO7gU">Einer; Einziger</w>
<w xml:lang="de" corresp="src:tlaIBUBd7Yfm0FPZUCemQUYfCB1kWI">führen, leiten</w>
<w xml:lang="de" corresp="src:tlaIBUBd8RTF36cg0LBmujCbzEBdf0">sie [pron. suff. 3. pl.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd5pe3tbeY0sQhV6U26dKdCc">auf, über, vor, hinter [lok.]</w>
<w xml:lang="de" corresp="src:tlaIBUBdQRpscOfDEelqLEcmUwqa3o">Stätte; Hügel</w>
<w xml:lang="de" corresp="src:tlaIBUBdwr4E8KdRU7Bve2lmavTiVs">GN/Osiris</w>
</s>
