<ab>
<s corresp="src:tlaIBUBdwJUJsoa6kDzkdWCqlKv5l4">
<w xml:lang="de" corresp="src:tlaIBUBd6x4NwM8tUQ2u3zuEJnzOW4">Spruch</w>
<w xml:lang="de" corresp="src:tlaIBUBdzTmVdYZyUNCrceSItqK6tU">[Gen.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd5cZ6VkbUURohKm9z6S6cPM">gelangen nach, kommen zu; erreichen</w>
<w xml:lang="de" corresp="src:tlaIBUBdW7Fkz1yk0WahIwWQvzMStI">in, zu, an, aus [lokal]</w>
<w xml:lang="de" corresp="src:tlaIBUBdwNSF6YVQkVFrOejdmphH68">ON/Rosetau</w>
<w xml:lang="de" corresp="src:tlaIBUBd07BB7w1CkogpIWPr49KBsQ">durch, seitens jmds.</w>
<w xml:lang="de" corresp="src:tlaIBUBd4Yo3k7Jx0jajYcHiIaLHpk">gerechtfertigt, Seliger</w>
</s>
<s corresp="src:tlaIBUBdyr3ZaP21k6JlfkFF81xy9Y">
<w xml:lang="de" corresp="src:tlaIBUBd6S00sqAKk5jmfdccO53MVk">ich, [pron. abs. 1. sg.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd8UJ4jUcX0RajGV5PeW5wkk">gebären</w>
<w xml:lang="de" corresp="src:tlaIBUBd7eSzQFtzk4CjT0VxyH7Wk0">in, zu, an, aus [lokal]</w>
<w xml:lang="de" corresp="src:tlaIBUBdWvEcIANQkPimJB1Oci9oas">ON/Rosetau</w>
</s>
<s corresp="src:tlaIBUBdWB0C9PExE5JiWqZv4hEobA">
<w xml:lang="de" corresp="src:tlaIBUBd2z3fYFOqEethrA3DbNgxdk">geben</w>
<w xml:lang="de" corresp="src:tlaIBUBdxGySXgP40EVmDq4U5s3Rbs">[Dat.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd2vC6kOs80wUo1hhI21nmyQ">ich [pron. suff. 1. sg.]</w>
<w xml:lang="de" corresp="src:tlaIBUBdziA8Rz7UkSQvRHonxsnN3o">Herrlichkeit, Nützliches</w>
<w xml:lang="de" corresp="src:tlaIBUBdyQNl55vqUPvqmPQKroZfaM">[Einführg. d. log. Subj. b. Pass.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd9qPYbAj4UNdgonY8fY9kg0">Herr, Besitzer von etw.</w>
<w xml:lang="de" corresp="src:tlaIBUBdwiKMJv2VUFqvdSCSnMTwu8">Würdenzeichen, Würde</w>
<w xml:lang="de" corresp="src:tlaIBUBdxdsBQQVj0iPiHMYMNNZozc">in (Freude), als (Gekochtes) [Zustand,Weise]</w>
<w xml:lang="de" corresp="src:tlaIBUBd6HGZv9HQER8gSAufT3dHyU">Reinigung, Reinheit, Reinigungsopfer</w>
<w xml:lang="de" corresp="src:tlaIBUBdxuCbxqkC0gVn4QYPvXyNAY">GN/Osiris</w>
</s>
<s corresp="src:tlaIBUBdygTS7xHsE7uiXLeDBUccQQ">
<w xml:lang="de" corresp="src:tlaIBUBdwC3V7v0LUWMoB0BNQmb2X0">empfangen, nehmen</w>
<w xml:lang="de" corresp="src:tlaIBUBd9Go122CEk4Pv9bQ87zJtmw">das Alter</w>
<w xml:lang="de" corresp="src:tlaIBUBdWXywvIp7UZMoyWLqk9qRcQ">in, zu, an, aus [lokal]</w>
<w xml:lang="de" corresp="src:tlaIBUBd3qHGOuaT06NtCTaYqyC9WI">ON/Rosetau</w>
<w xml:lang="de" corresp="src:tlaIBUBd9GZS8ob4UixqebqhepETxQ">als, wenn [Konjunktion]</w>
<w xml:lang="de" corresp="src:tlaIBUBd4DdgqV5H0IZjzxkEX4NS54">führen, leiten</w>
<w xml:lang="de" corresp="src:tlaIBUBd8v3PTqSDEoDrhplGXuYBsQ">GN/Osiris</w>
<w xml:lang="de" corresp="src:tlaIBUBd2QPPAD890yrhMweBttR2kc">auf, über, vor, hinter [lok.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd9buw2I1cUAFqyj91Di5gRU">Stätte; Hügel</w>
<w xml:lang="de" corresp="src:tlaIBUBd68M4jCM7E3fpMFrxv7UBEY">GN/Osiris</w>
</s>
<s corresp="src:tlaIBUBd9ZTOWTM4kTahICBTCqOERU">
<w xml:lang="de" corresp="src:tlaIBUBd7FFGQ5QP02TjNsWJBy0ba4">ich, [pron. abs. 1. sg.]</w>
<w xml:lang="de" corresp="src:tlaIBUBdzrwhuhhNkJ6q9VSNpWaGiU">Einziger</w>
<w xml:lang="de" corresp="src:tlaIBUBdz9qpjrpd0MHh06dWyqWYlc">führen, leiten</w>
<w xml:lang="de" corresp="src:tlaIBUBdzB9eqX3TEMikBikAyNcJnI">sie [pron. suff. 3. pl.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd6Z9KjSLZU6pmzJ7SDTUjt0">auf, über, vor, hinter [lok.]</w>
<w xml:lang="de" corresp="src:tlaIBUBd9zaiJUlX0UMuQAJPtKQZL4">Stätte; Hügel</w>
<w xml:lang="de" corresp="src:tlaIBUBdQHloT2lQ0amqljbu1YJpPg">GN/Osiris</w>
</s>
